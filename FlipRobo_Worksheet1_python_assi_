   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "bc695b9b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the string :madam\n",
      "String is Palindrome.\n"
     ]
    }
   ],
   "source": [
    "r=input(\"Enter the string :\")\n",
    "rev_r =r[::-1]\n",
    "\n",
    "if rev_r==r:\n",
    "    print(\"String is Palindrome.\")\n",
    "else:\n",
    "    print(\"String is not Palindrome.\")"
   ]
